{
  "repositories"     : [
    {
      "type": "composer",
      "url" : "https://gest.unicaen.fr/packagist"
    }
  ],

  "name": "unicaen/oscar",
  "license": "proprietary",
  "description": "",

  "require": {
    "unicaen/app": "6.0.6",
    "unicaen/authentification": "6.0.3",
    "unicaen/privilege": "6.0.1",
    "unicaen/utilisateur": "6.0.4",
    "laminas/laminas-file": "^2.8",
    "oro/doctrine-extensions": "^1.2",
    "symfony/yaml": "^4.3",
    "fightbulc/moment": "^1.30",
    "cocur/slugify": "^3.2",
    "erusev/parsedown": "^1.7",
    "jacksay/php-file-extension": "^0.0.1",
    "elasticsearch/elasticsearch": "^7.2",
    "monolog/monolog": "^2.0",
    "phpoffice/phpspreadsheet": "^1.9",
    "dompdf/dompdf": "^0.8.3",
    "phpoffice/phpword": "*",
    "swiftmailer/swiftmailer": "^6.2",
    "symfony/console": "*",
    "ext-gettext": "*",
    "ext-json": "*",
    "guzzlehttp/guzzle": "^7.0"
