{
  "repositories"     : [
    {
      "type": "composer",
      "url" : "https://gest.unicaen.fr/packagist"
    }
  ],

  "name": "unicaen/oscar",
  "license": "proprietary",
  "description": "",

  "require": {
    "unicaen/app": "^6",
    "unicaen/authentification": "^6",
    "unicaen/utilisateur": "^6",
    "unicaen/privilege": "^6",
    "laminas/laminas-file": "^2.8",
    "oro/doctrine-extensions": "^1.2",
    "symfony/yaml": "^4.3",
    "fightbulc/moment": "^1.30",
    "cocur/slugify": "^3.2",
    "erusev/parsedown": "^1.7",
    "jacksay/php-file-extension": "^0.0.1",
    "elasticsearch/elasticsearch": "^7.2",
    "monolog/monolog": "^2.0",
    "dompdf/dompdf": "^0.8.3",
    "swiftmailer/swiftmailer": "^6.2",
    "symfony/console": "*",
    "guzzlehttp/guzzle": "^7.0",
    "phpoffice/phpspreadsheet": "^1.29",
    "phpoffice/phpword": "^1.1"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {
    "phpunit/phpunit": "*"
  },
  "config": {
    "github-protocols": ["https", "ssh"],
    "secure-http": false,
    "allow-plugins": {
      "laminas/laminas-dependency-plugin": true
    }
  },
  "autoload": {
    "psr-0": {
      "Oscar": ["module/Oscar/src/", "local/src/"],
      "PhpOffice": "lib/PhpSpreadsheet/src"
    }
  },
  "scripts": {
    "test": "phpunit -c tests"
  }
}
